    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="subproCheckoutLoginMsg">
                <display-name xml:lang="x-default">Subpro Checkout Login Message</display-name>
                <description xml:lang="x-default">Message informing customer that he/she needs to be logged in as a registered customer to proceed checkout with SubPro Subscription products</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>You need to be logged in as a registered customer to proceed checkout with SubPro Subscription products</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="subproApplePayLoginMsg">
                <display-name xml:lang="x-default">Subpro Apple Pay Login Message</display-name>
                <description xml:lang="x-default">Message informing customer that he/she needs to be logged in as a registered customer to use Apple Pay with SubPro Subscription products</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>You need to be logged in as a registered customer to use Apple Pay with SubPro Subscription products</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="subproCreditCardRequirementMsg">
                <display-name xml:lang="x-default">Subpro Credit Card Requirement Message</display-name>
                <description xml:lang="x-default">Error message noting that a credit card is required for subscriptions</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>You need a credit card to pay for SubPro subscription</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="subproOrderProcessingErrorMail">
                <display-name xml:lang="x-default">Subscribe Pro Order Processing Error Mail</display-name>
                <description xml:lang="x-default">An email address that order subscriptions processing failures will be sent to</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="subscribeProWidgetScriptUrl">
                <display-name xml:lang="x-default">Subscribe Pro Widget Script Url</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>https://widgets.subscribepro.com/subscribepro-widgets.min.js</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="apiBaseUrl">
                <display-name xml:lang="x-default">Subscribe Pro API Base Url</display-name>
