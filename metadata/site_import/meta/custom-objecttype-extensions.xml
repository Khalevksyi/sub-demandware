<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <custom-type type-id="subProAccessToken">
        <display-name xml:lang="x-default">Sub Pro Access Token</display-name>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>organization</storage-scope>
        <key-definition attribute-id="accessToken">
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="expiresOn">
                <display-name xml:lang="x-default">expiresOn</display-name>
                <type>double</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="token">
                <display-name xml:lang="x-default">Token</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="tokens">
                <display-name xml:lang="x-default">Tokens</display-name>
                <attribute attribute-id="accessToken"/>
                <attribute attribute-id="token"/>
                <attribute attribute-id="expiresOn"/>
            </attribute-group>
        </group-definitions>
    </custom-type>

</metadata>
